import time
from threading import Thread
from datetime import datetime

def get_thread(thread_name):
	time.sleep(1)
	print(f'Thread № {thread_name} started!')
lst = ['john','bill','tom','jack','barbara']
# последовательно
for i in range(len(lst)):
	get_thread(lst[i])

t1 = datetime.now()
print('Последовательно: ', (datetime.now() - t1).microseconds)

# параллельно
threads = [Thread(target = get_thread, args=(lst[i],)) for i in range(len(lst))]

for t in threads:
	t.start()

for t in threads:
	t.join()

t2 = datetime.now()
print('Параллельно: ', (datetime.now() - t2).microseconds)

